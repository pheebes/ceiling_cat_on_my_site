<p id="notice"><%= notice %></p>

<h1>Listing Daily Logs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Day</th>
      <th>Body</th>
      <th>Ranking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_logs.each do |daily_log| %>
      <tr>
        <td><%= daily_log.title %></td>
        <td><%= daily_log.day %></td>
        <td><%= daily_log.body %></td>
        <td><%= daily_log.ranking %></td>
        <td><%= link_to 'Show', daily_log %></td>
        <td><%= link_to 'Edit', edit_daily_log_path(daily_log) %></td>
        <td><%= link_to 'Destroy', daily_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily log', new_daily_log_path %>
